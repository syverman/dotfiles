{
    "aspell.check": "on",
    "aspell.lang": "us",
    "autosave": 240,
    "colorscheme": "simple",
    "lsp.autocompleteDetails": true,
    "lsp.ignoreMessages": "LS message1 to ignore|LS message 2 to ignore|...",
    "lsp.ignoreTriggerCharacters": "completion,signature",
    "lsp.rust": "{\"cargo\": {\"buildScripts\": {\"enable\": true} }, \"procMacro\": {\"enable\": true} }",
    "lsp.server": "python=pylsp,go=gopls,typescript=deno lsp,javascript=deno lsp,markdown=deno lsp,json=deno lsp,jsonc=deno lsp,rust=rust-analyzer,lua=lua-lsp,c++=clangd",
    "lsp.typescript": "{\"enable\":true}",
    "relativeruler": true,
    "savecursor": true,
    "saveundo": true,
    "softwrap": true,
    "wordwrap": true
}
